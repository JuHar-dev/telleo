{
  "name": "telleo",
  "version": "1.0.0",
  "description": "",
  "main": "./build/src/app.js",
  "scripts": {
    "build": "npm run clean && npx tsc --build",
    "dev": "cross-env NODE_ENV=development nodemon ./src/app.ts",
    "dev-build": "npm run build && node ./build/src/app.js",
    "clean": "rimraf ./build",
    "test": "cross-env NODE_ENV=test mocha -r ts-node/register --watch-extensions ts 'test/**/*.spec.ts' --exit --color --timeout 10000",
    "integration-test": "cross-env NODE_ENV=test mocha -r ts-node/register --watch-extensions ts 'test/integration_tests/**/*.integration.spec.ts' --exit --color --timeout 10000",
    "unit-test": "cross-env NODE_ENV=test mocha -r ts-node/register 'test/unit_tests/**/*.unit.spec.ts' --exit --color --timeout 10000",
    "coverage": "npm run dependency_graph && rimraf ./.nyc_output coverage && npm run build && npx nyc npm run test && npm run show_coverage",
    "start": "node build/src/app.js",
    "postinstall": "npm run tsc",
    "tsc": "tsc",
    "show_coverage": "start ./coverage/index.html",
    "playground": "nodemon ./src/playground.ts",
    "dependency_graph": "npm run build && rimraf dependency_graph && mkdir dependency_graph && madge --image dependency_graph/graph.png --extensions js ./build",
    "dependencies": "madge --extensions ts ./src/v1/data"
  },
  "engines": {
    "node": "16.13.1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@tsconfig/node14": "^1.0.1",
    "@types/bcrypt": "^5.0.0",
    "@types/chai": "^4.2.22",
    "@types/chai-http": "^4.2.0",
    "@types/colors": "^1.2.1",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/lodash": "^4.14.177",
    "@types/mocha": "^9.0.0",
    "@types/mongodb-memory-server": "^2.3.0",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.11",
    "@types/passport": "^1.0.7",
    "@types/passport-http-bearer": "^1.0.37",
    "@types/pino": "^7.0.5",
    "@types/pino-http": "^5.8.1",
    "@types/socket.io": "^3.0.2",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-security": "^1.4.0",
    "madge": "^5.0.1",
    "mocha": "^9.1.3",
    "mongodb-memory-server": "^8.0.4",
    "nodemon": "^2.0.15",
    "nyc": "^15.1.0",
    "prettier": "^2.5.0",
    "prettier-eslint": "^13.0.0",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.21",
    "ts-mockito": "^2.6.1"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "colors": "^1.4.0",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "funpjs": "^1.0.6",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mongoose": "^6.0.14",
    "morgan": "^1.10.0",
    "passport": "^0.5.0",
    "passport-http-bearer": "^1.0.1",
    "pino-http": "^6.3.3",
    "socket.io": "^3.0.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2",
    "winston": "^3.3.3"
  }
}
